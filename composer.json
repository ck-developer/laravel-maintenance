{
    "name": "ck/laravel-maintenance",
    "description": "Laravel 5's maintenance mode",
    "keywords": [
        "laravel",
        "laravel5",
        "l5",
        "maintenance"
    ],
    "homepage": "https://github.com/ck-developer/laravel-maintenance",
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Khedhiri",
            "email": "claude@khedhiri.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/config": "5.1.*",
        "illuminate/support": "5.1.*",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "3.1.*"
    },
    "autoload": {
        "psr-4": {
            "Mwc\\Laravel\\Maintenance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mwc\\Laravel\\Maintenance\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
